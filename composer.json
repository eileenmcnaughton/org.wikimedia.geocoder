{
  "name": "civicrm/geocoder",
  "type": "civicrm-ext",
  "description": "Geocoding for CiviCRM based on geocoder library",
  "license": "AGPL-3.0",
  "authors": [
    {
      "name": "Eileen McNaughton",
      "email": "eileen@fuzion.co.nz"
    }
  ],
  "minimum-stability": "dev",
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/eileenmcnaughton/civicrm-data-table-provider.git"
    }
  ],
  "require": {
    "php-http/discovery":"^1.17",
    "psr/http-client-implementation":"*",
    "psr/http-message-implementation":"*",
    "geocoder-php/free-geoip-provider": "^4.1",
    "php-http/message": "^1.6",
    "geocoder-php/nominatim-provider": "5.6.*",
    "geocoder-php/mapquest-provider": "^4",
    "geocoder-php/google-maps-provider": "^4.4",
    "wikimedia/civicrm-data-table-provider": "^5",
    "geo6/geocoder-php-addok-provider": "^1",
    "geocoder-php/here-provider": "^0.6",
    "geocoder-php/common-http": "4.5.*"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": false
    }
  }
}
